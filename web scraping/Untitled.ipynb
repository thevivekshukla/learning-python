   "source": [
    "from bs4 import BeautifulSoup\n",
    "import requests\n",
    "\n",
    "r = requests.get('https://whentosurf.co/')\n",
    "c = r.content\n",
    "\n",
    "soup = BeautifulSoup(c, 'html.parser')\n",
    "\n",
    "#print(soup.prettify())\n",
    "\n",
    "anchor = soup.find_all('a')\n",
    "print(len(anchor))\n",
    "for a in anchor:\n",
    "    print(a.get('href'))"
   ]
